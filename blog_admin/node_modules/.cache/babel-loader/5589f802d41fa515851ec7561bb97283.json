-item>\r\n            <el-select\r\n              v-model=\"article.categoryId\"\r\n              placeholder=\"栏目\"\r\n            >\r\n              <el-option\r\n                v-for=\"item in categories\"\r\n                :key=\"item.id\"\r\n                :label=\"item.name\"\r\n                :value=\"item.id\"\r\n              />\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row>\r\n        <el-col :span=\"24\">\r\n          <el-form-item label=\"标题\" label-width=\"60px\">\r\n            <el-input\r\n              class=\"title-input\"\r\n              placeholder=\"输入标题\"\r\n              v-model=\"article.title\"\r\n            />\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row>\r\n        <el-col :span=\"24\">\r\n          <el-form-item label=\"摘要\" label-width=\"60px\">\r\n            <el-input\r\n              class=\"summary-input\"\r\n              type=\"textarea\"\r\n              :autosize=\"{ minRows: 1, maxRows: 3}\"\r\n              v-model=\"article.summary\"\r\n              placeholder=\"输入摘要\"\r\n            />\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row>\r\n        <el-col :span=\"6\" style=\"margin-right: 50px\">\r\n          <el-form-item label=\"作者\" label-width=\"60px\">\r\n            <el-input\r\n              placeholder=\"输入作者\"\r\n              v-model=\"article.author\"\r\n            />\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item label=\"标签\" label-width=\"60px\">\r\n            <el-select\r\n              style=\"width: 300px;\"\r\n              v-model=\"article.tagNames\"\r\n              multiple\r\n              filterable\r\n              allow-create\r\n              placeholder=\"添加文章标签\"\r\n            >\r\n              <el-option\r\n                v-for=\"item in tags\"\r\n                :key=\"item.id\"\r\n