;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Home.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div class=\"home\">\n    <el-container>\n      <el-header>\n        <div class=\"home-title\">博客管理平台</div>\n        <div>\n          <el-dropdown @command=\"handleCommand\">\n            <span class=\"el-dropdown-link user-info\">\n              {{currentUsername}}<i class=\"el-icon-arrow-down el-icon--right user-info\"></i>\n            </span>\n            <el-dropdown-menu slot=\"dropdown\">\n              <el-dropdown-item command=\"myArticle\">我的文章</el-dropdown-item>\n              <el-dropdown-item command=\"logout\">退出登录</el-dropdown-item>\n            </el-dropdown-menu>\n          </el-dropdown>\n        </div>\n      </el-header>\n      <el-container>\n        <el-aside>\n          <el-menu\n            default-active=\"this.$router.currentRoute.path\"\n            router>\n            <template v-for=\"(item, index) in routes\">\n              <el-submenu\n                v-if=\"item.children.length>1\"\n                :index=\"item.path\"\n                :key=\"index\"\n              >\n                <template slot=\"title\">\n                  <i :class=\"item.meta.icon\"></i>\n                  <span>{{item.name}}</span>\n                </template>\n                <el-menu-item\n                  v-for=\"child in children(item)\"\n                  :index=\"resolvePath(item,child)\"\n                  :key=\"child.path\"\n                >\n                  <i :class=\"child.meta.icon\"></i>\n                  <span slot=\"title\">{{child.meta.title}}</span>\n                </el-menu-item>\n              </el-submenu>\n              <template v-if=\"item.children.length===1\">\n                <el-menu-item\n                  :index=\"resolvePath(item,item.children[0])\"\n                  :key=\"item.path\"\n                >\n                  <i :class=\"item.children[0].meta.icon\"></i>\n                  <span slot=\"title\">{{item.children[0].name}}</span>\n                </el-menu-item>\n              </template>\n            </template>\n          </el-menu>\n        </el-aside>\n        <el-main>\n          <el-breadcrumb separator-class=\"el-icon-arrow-right\">\n            <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n            <el-breadcrumb-item v-text=\"this.$router.currentRoute.name\"></el-breadcrumb-item>\n          </el-breadcrumb>\n          <div class=\"view-container\">\n            <router-view></router-view>\n          </div>\n        </el-main>\n      </el-container>\n    </el-container>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  name: 'home',\n  computed: {\n    currentUsername () {\n      return this.$store.state.user.name\n    },\n    routes () {\n      return this.$router.options.routes.filter(item => !item.hidden)\n    }\n  },\n  methods: {\n    handleCommand (command) {\n      if (command === 'myArticle') {\n        this.$router.push({ path: '/article/list' })\n      } else if (command === 'logout') {\n        this.$confirm('确认退出登录', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning'\n        })\n          .then(() => {\n            this.$store.dispatch('user/logout')\n              .then(() => {\n                this.$router.replace({ path: '/login' })\n              })\n          })\n      }\n    },\n    resolvePath (item, child) {\n      return item.path + '/' + child.path\n    },\n    children (item) {\n      return item.children.filter(child => !child.hidden)\n    }\n  }\n}\n</script>\n\n<style>\n  body {\n    margin: 0;\n    padding: 0;\n  }\n  .el-header {\n    background-color: #20a0ff;\n    color: #333;\n    text-align: center;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n  }\n  .home-title {\n    font-size: 22px;\n    color: white;\n    display: inline;\n  }\n  .user-info {\n    color: white;\n    cursor: pointer;\n  }\n  .el-icon-arrow-down {\n    font-size: 12px;\n  }\n  .view-container {\n    margin-top: 30px;\n    padding: 0 20px;\n  }\n</style>\n"]}]}